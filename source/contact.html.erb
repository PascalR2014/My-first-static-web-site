	<div class="row">
		<div id="fondform">
			<div class ="col-md-6 col-md-offset-3">
				<div id="formulaire">
					<form>
						<div id="boxnom">
							<p>Votre Nom:</p>
							<input type="text" oval="Nom*" name="Nom" placeholder="Nom*" required="required"/>
						</div>
						<div id="boxprenom">
							<p>Votre Prénom:</p>
							<input type="text" oval="Prénom*" name="Prenom" placeholder="Prénom*" required="required"/>
